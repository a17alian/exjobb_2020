doctype html
html
  head
    title= title
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/ol.css')
    link(rel='stylesheet', href='https://cdn.jsdelivr.net/gh/openlayers/openlayers.github.io@master/en/v6.2.1/css/ol.css' type='text/css')
    script(src="https://cdn.jsdelivr.net/gh/openlayers/openlayers.github.io@master/en/v6.2.1/build/ol.js") 
    script(src="https://code.jquery.com/jquery-2.2.3.min.js")    
    script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js") 

body
  block content
    div(class='map' id='map')
      #controlls
        div
          input(type="radio" id="heatmap" name="layers")
          label(for="Heatmap") Heatmap
        div
          input(type="radio" id="markers" value="Markers" name="layers")
          label(for="Markers") Markers
      #popup
      #fab-btn(onclick='showInput()') 
      #fab-input   
          span#exit-btn(onclick='showInput()') X
          h2#load_time 
          h5#data
          h5#tile_data 
          button#send_data Send Data
  script(src='/javascripts/ol.js')
  script(src='/javascripts/shared.js')  
 
